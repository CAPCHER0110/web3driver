# 服务端口
SERVER_PORT=8080

# 数据库配置 (DSN)
# 格式: user:pass@tcp(host:port)/dbname?charset=utf8mb4&parseTime=True&loc=Local
DB_DSN="user:password@tcp(127.0.0.1:3306)/web3?charset=utf8mb4&parseTime=True&loc=Local"

# JWT 密钥 (生成命令: openssl rand -hex 32)
JWT_SECRET="my_super_secret_key"

# Pinata IPFS Token (JWT)
PINATA_JWT="jwttoken"
